pipeline {
    agent any
    stages {
        stage('Initializing') {
            steps {
                echo 'Initializing Jarvis'
                echo 'Welcome'
            }
        }
        stage('Build') {
            steps {
                echo 'We are in Build stage'
            }
        }
        stage('Deploy'){
            steps {
                echo 'We are in Deploy'
            }
        }
    }
}